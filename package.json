{
  "name": "py_server",
  "version": "0.0.0",
  "scripts": {
    "activate_env": "source activate & conda activate server_py",
    "dev": "conda activate pc_py & python src/server_main.py",
    "debug": "conda activate pc_py & python src/server_main.py",
    "build": "conda activate server_py & pyinstaller --distpath=dist --workpath=build  server_main.spec",
    "export_pyproject_to_requirements": "uv export --format=requirements.txt --no-hashes --no-annotate --no-header > requirements.txt"
  }
}